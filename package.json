{
  "name": "kevoree-chan-remotews",
  "version": "5.3.3-beta.1",
  "main": "./lib/RemoteWSChan.js",
  "description": "A Kevoree chan that uses an external remote WebSocket broadcast server to share messages",
  "kevoree": {
    "namespace": "kevoree"
  },
  "scripts": {
    "prepublish": "npm run lint && grunt",
    "postpublish": "grunt publish",
    "lint": "eslint lib browser.js"
  },
  "keywords": [
    "kevoree-std-lib",
    "kevoree",
    "library",
    "chan"
  ],
  "browser": {
    "ws": "./browser/ws-shim.js"
  },
  "dependencies": {
    "kevoree-entities": "^9.0.0",
    "uuid": "^3.0.1",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "eslint": "^3.12.2",
    "grunt": "^1.0.1",
    "grunt-kevoree": "^5.9.1",
    "grunt-kevoree-genmodel": "^3.0.1",
    "grunt-kevoree-registry": "^3.0.3",
    "grunt-webpack": "^1.0.18",
    "json-loader": "^0.5.4",
    "load-grunt-tasks": "^3.5.2",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kevoree/kevoree-js-chan-remotews.git"
  },
  "author": "Maxime Tricoire <max.tricoire@gmail.com>",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/kevoree/kevoree-js-chan-remotews/issues"
  },
  "homepage": "https://github.com/kevoree/kevoree-js-chan-remotews",
  "eslintConfig": {
		"root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ]
  }
}
